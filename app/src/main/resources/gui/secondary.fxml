<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="533.0" prefWidth="559.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.SecondaryController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label text="Choose the program">
         <font>
            <Font name="Constantia Bold" size="24.0" />
         </font>
      </Label>
      <ListView fx:id="prgList" editable="true" onMouseClicked="#programSelection" prefHeight="407.0" prefWidth="519.0" />
   </children>
</VBox>
